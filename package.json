{
  "name": "cloud-frontend",
  "author": "Deramecourt Mathurin",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "build-start": "export NODE_ENV=test && next build && next start",
    "gen": "graphql-codegen --config codegen.yml",
    "cypress": "./node_modules/.bin/cypress open",
    "cypress:open": "./node_modules/.bin/cypress open",
    "cypress:run": "./node_modules/.bin/cypress run",
    "cypress:run:novid": "./node_modules/.bin/cypress run --config video=false",
    "docker:build": "sudo docker build -t mderam/cloud-frontend .",
    "docker:push": "sudo docker push mderam/cloud-frontend",
    "docker:build-push": "npm run docker:build && npm run docker:push"
  },
  "dependencies": {
    "@graphql-codegen/cli": "^2.6.2",
    "@urql/exchange-graphcache": "^4.3.6",
    "@urql/exchange-multipart-fetch": "^0.1.14",
    "axios": "^0.27.2",
    "classnames": "^2.3.1",
    "graphql": "^16.4.0",
    "graphql-tag": "^2.12.6",
    "next": "latest",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "urql": "^2.2.0",
    "uuid": "^8.3.2",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.6.2",
    "@graphql-codegen/typescript": "2.4.8",
    "@graphql-codegen/typescript-operations": "2.3.5",
    "@graphql-codegen/typescript-urql": "^3.5.7",
    "@graphql-codegen/urql-introspection": "2.1.1",
    "@types/node": "17.0.4",
    "@types/react": "17.0.38",
    "@types/uuid": "^8.3.4",
    "@types/validator": "^13.7.2",
    "@urql/core": "^2.4.4",
    "@urql/devtools": "^2.0.3",
    "autoprefixer": "^10.4.0",
    "cypress": "^10.0.3",
    "cypress-recurse": "^1.20.0",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "prettier-plugin-tailwindcss": "^0.1.1",
    "smtp-tester": "^1.2.0",
    "tailwindcss": "^3.0.7",
    "typescript": "4.5.4"
  }
}
